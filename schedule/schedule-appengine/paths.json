{
  "js": [
    "src/main/client/**/*.module.js",
    "src/main/client/**/*.js"
  ],
  "test": {
    "js": [
      "src/test/client/**/*.js",
      "!src/test/client/**/karma.conf*.js"
    ],
    "karmaConfig": "src/test/client/karma.conf.ci.js"
  },
  "css": [
    "src/main/stylesheet/angular-material-theme.css",
    "src/main/client/jasify/directives/a-button/a-button.css",
    "src/main/client/jasify/directives/mask/mask.css",
    "src/main/stylesheet/styles.less"
  ],
  "watchCss": [
    "src/main/client/jasify/directives/**/*.*",
    "src/main/stylesheet/**/*.*"
  ],
  "html": [
    "src/main/client/*.html"
  ],
  "staticHtml": [
    "src/main/html/**/*.html"
  ],
  "partials": [
    "src/main/client/jasify/**/*.html"
  ],
  "dependencies": {
    "js": {
      "boot": [
        "html5shiv/dist/html5shiv.min.js",
        "respond/dest/respond.min.js"
      ],
      "main": [
        "jquery/dist/jquery.js",
        "jquery-bootgrid/dist/jquery.bootgrid.js",
        "bootstrap/dist/js/bootstrap.js",
        "angular/angular.js",
        "angular-material/angular-material.js",
        "angular-aria/angular-aria.js",
        "angular-animate/angular-animate.js",
        "angular-route/angular-route.js",
        "angular-resource/angular-resource.js",
        "angular-messages/angular-messages.js",
        "angular-cookies/angular-cookies.js",
        "angular-sanitize/angular-sanitize.js",
        "angular-local-storage/dist/angular-local-storage.js",
        "angular-bootstrap/ui-bootstrap-tpls.js",
        "angular-filter/dist/angular-filter.js",
        "ui-bootstrap-datetime-picker/dist/datetime-picker.js",
        "spin.js/spin.js",
        "angular-spinner/angular-spinner.js"
      ],
      "test": [
        "angular-mocks/angular-mocks.js"
      ]
    },
    "css": {
      "main": [
        "ionicons/css/ionicons.min.css",
        "font-awesome/css/font-awesome.min.css",
        "angular-material/angular-material.css",
        "bootstrap/dist/css/bootstrap.css",
        "jquery-bootgrid/dist/jquery.bootgrid.css"
      ]
    }
  },
  "buildTemplate": "target/schedule-appengine-@VERSION@/build",
  "bower": "target/bower_components",
  "symBuild": "src/main/html/build"
}