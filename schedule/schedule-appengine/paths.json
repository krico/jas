{
  "js": [
    "src/main/client/**/*.module.js",
    "src/main/client/**/*.js"
  ],
  "test": {
    "js": [
      "src/test/client/**/*.js",
      "!src/test/client/**/karma.conf*.js"
    ],
    "karmaConfig": "src/test/client/karma.conf.ci.js"
  },
  "css": [
    "src/main/stylesheet/styles.less"
  ],
  "watchCss": [
    "src/main/stylesheet/**/*.*"
  ],
  "html": [
    "src/main/client/*.html"
  ],
  "staticHtml": [
    "src/main/html/**/*.html"
  ],
  "partials": [
    "src/main/client/jasify/**/*.html"
  ],
  "dependencies": {
    "js": {
      "boot": [
        "html5shiv/dist/html5shiv.min.js",
        "respond/dest/respond.min.js"
      ],
      "main": [
        "angular/angular.min.js",
        "angular-route/angular-route.min.js",
        "angular-resource/angular-resource.min.js",
        "angular-messages/angular-messages.min.js",
        "angular-cookies/angular-cookies.min.js",
        "angular-sanitize/angular-sanitize.min.js",
        "angular-local-storage/dist/angular-local-storage.min.js",
        "angular-bootstrap/ui-bootstrap-tpls.min.js",
        "ui-bootstrap-datetime-picker/dist/datetime-picker.min.js",
        "spin.js/spin.js",
        "angular-spinner/angular-spinner.min.js"
      ],
      "test": [
        "angular-mocks/angular-mocks.js"
      ]
    },
    "css": {
      "main": [
        "ionicons/css/ionicons.min.css",
        "font-awesome/css/font-awesome.min.css"
      ]
    }
  },
  "build": "target/schedule-appengine-@VERSION@/build",
  "bower": "target/bower_components",
  "symBuild": "src/main/html/build"
}